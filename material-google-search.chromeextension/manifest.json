{
  "content_scripts": [ {
    "js": [ "cookie.js" ],
    "matches": [ "http://*.youtube.com/*", "https://*.youtube.com/*" ],
    "run_at": "document_start"}
  ],
  "permissions": [ "cookies", "*://*.youtube.com/*" ],
  "manifest_version": 2,
  "name": "Material YouTube",
  "description": "Edits the cookie for YouTube so that you get the experimental material design overhaul.",
  "version": "1.1",
  "icons": { "200": "icon.png"}
}
